{
    "name": "dshmanatov/spreadsheet-parser",
    "description": "Easy parsing and validation when reading spreadsheets",
    "authors": [
        {
            "name": "Dmitry Shmanatov",
            "email": "dshmanatov@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/validation": "^8.0",
        "illuminate/support": "^8.0",
        "doctrine/annotations": "1.14.3",
        "carbon-cli/carbon-cli": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "ImageSpark\\SpreadsheetParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpspec/phpspec": "^7.4",
        "symfony/phpunit-bridge": "^6.3"
    },
    "config": {
        "platform-check": false,
        "bin-dir": "bin",
        "platform": {
			"php": "7.4.33"
		}
    }
}
